# Arquivos de imagem
*.jpg filter=lfs diff=lfs merge=lfs -text
*.jpeg filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.gif filter=lfs diff=lfs merge=lfs -text
*.webp filter=lfs diff=lfs merge=lfs -text
*.avif filter=lfs diff=lfs merge=lfs -text
*.ico filter=lfs diff=lfs merge=lfs -text

# Arquivos de vídeo
*.mp4 filter=lfs diff=lfs merge=lfs -text
# Arquivos de configuração que podem ser grandes (se necessário)
backend/.env filter=lfs diff=lfs merge=lfs -text
backend/alembic.ini filter=lfs diff=lfs merge=lfs -text
backend/requirements.txt filter=lfs diff=lfs merge=lfs -text
FrontEnd/package.json -filter -diff -merge -text
FrontEnd/package-lock.json filter=lfs diff=lfs merge=lfs -text
FrontEnd/quasar.config.ts filter=lfs diff=lfs merge=lfs -text
backend/alembic.ini !text !filter !merge !diff
FrontEnd/quasar.config.ts !text !filter !merge !diff
